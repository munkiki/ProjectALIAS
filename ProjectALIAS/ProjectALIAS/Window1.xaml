<Window x:Class="ProjectALIAS.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectALIAS"
        mc:Ignorable="d"
        Title="Window1" Height="500" Width="600" Background="Black" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Name="z" Click="backToMainWindow" Margin ="10,389,382,10" Foreground="#FFB62525" Width = "200" Height ="70" Background ="Black" FontSize="36" FontFamily="Times New Roman" FontWeight="Bold" BorderBrush="Black">
            Back</Button>
        <Button Name="x" Click="startNewGame" Margin ="382,389,10,10" Foreground="#FF0AB021" Width = "200" Height ="70" Background ="Black" FontSize="36" FontFamily="Times New Roman" FontWeight="Bold" BorderBrush="Black">
            Begin</Button>
        <TextBox Margin="10,10,405,424" FontSize="22" Background="Black" Foreground="#FF5267EE" FontFamily="Times New Roman" IsReadOnly="True" BorderBrush="Black" FontWeight="Bold">Number of teams
        </TextBox>
        <RadioButton Margin="23,50,501,389" Name="TeamButton1" Foreground="White" IsChecked="True" Content="2"/>
        <RadioButton Margin="119,50,405,389" Name="TeamButton2" Foreground="White" Content="3"/>
        <RadioButton Margin="23,85,501,355" Name="TeamButton3" Foreground="White" Content="4"/>
        <RadioButton Margin="119,85,405,355" Name="TeamButton4" Foreground="White" Content="5"/>
    </Grid>
</Window>
